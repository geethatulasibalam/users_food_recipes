<div class="row">
	<div class="offset-3 col-md-8">
		<%- if controller_name != 'sessions' %>
			<span class="have-text">Already have an account?</span>
	  	<%= link_to "Sign In", new_session_path(resource_name),class: "links" %><br />
		<% end %>
	</div>
</div>
<div class="row">
	<div class="offset-3 col-md-8">
		<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
		  <span class="have-text">Don't have an account?</span>
			<%= link_to "Sign Up", new_registration_path(resource_name),class: "links" %><br />
		<% end %>
	</div>
</div>

